{
  "name": "Facebook Most Recent Posts",
  "version": "1.3.0.4",
  "description": "Clicking on [f] icon keeps newsfeed set to 'most recent'",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["most_recent.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
